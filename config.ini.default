[global]
svt_thermostat_temp=87

[secret]
server= <your server name>
ip_domoticz=
port=<8080>
username= <username if exists>
password= <password to access your domoticz server>

[feedback]
enable=false

[loggers]
keys=root

[handlers]
keys=consoleHandler

[formatters]
keys=

[logger_root]
level=INFO
handlers=consoleHandler

[handler_consoleHandler]
class=StreamHandler
level=INFO
args=(sys.stdout,)
